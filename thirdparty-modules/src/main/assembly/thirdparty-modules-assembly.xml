<assembly xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.3 http://maven.apache.org/xsd/assembly-1.1.3.xsd">
	<id>distribution</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<componentDescriptors>
		<componentDescriptor>components/camel-component.xml</componentDescriptor>
		<componentDescriptor>components/commons-collections4-component.xml</componentDescriptor>
		<componentDescriptor>components/commons-logging-component.xml</componentDescriptor>
		<componentDescriptor>components/commons-beanutils-component.xml</componentDescriptor>
		<componentDescriptor>components/commons-lang-component.xml</componentDescriptor>
		<componentDescriptor>components/commons-lang3-component.xml</componentDescriptor>
		<componentDescriptor>components/commons-codec-component.xml</componentDescriptor>
		<componentDescriptor>components/commons-math3-component.xml</componentDescriptor>
		<componentDescriptor>components/ehcache-component.xml</componentDescriptor>
		<componentDescriptor>components/httpcomponents-client-component.xml</componentDescriptor>
		<componentDescriptor>components/therore-concurrent.xml</componentDescriptor>
		<componentDescriptor>components/cxf-impl-component.xml</componentDescriptor>
		<componentDescriptor>components/joda-time-component.xml</componentDescriptor>
		<componentDescriptor>components/kie-component.xml</componentDescriptor>
		<componentDescriptor>components/marklogic-component.xml</componentDescriptor>
		<componentDescriptor>components/mvel-component.xml</componentDescriptor>
		<componentDescriptor>components/poi-component.xml</componentDescriptor>
		<componentDescriptor>components/springframework-component.xml</componentDescriptor>
		<componentDescriptor>components/xmlbeans-component.xml</componentDescriptor>
		<componentDescriptor>components/xstream-component.xml</componentDescriptor>
		<componentDescriptor>components/oracle-component.xml</componentDescriptor>
	</componentDescriptors>
	<fileSets>
		<fileSet>
			<filtered>true</filtered>
			<directory>src/main/resources</directory>
			<includes>
				<include>**/module.xml</include>
			</includes>
			<outputDirectory></outputDirectory>
		</fileSet>
	</fileSets>
</assembly>