<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="urn:jboss:module:1.1" name="org.apache.httpcomponents.client">
	<resources>
		<resource-root path="httpasyncclient-${httpasyncclient.version}.jar" />
		<resource-root path="httpclient-${httpclient.version}.jar" />
		<resource-root path="httpcore-${httpcore.version}.jar" />
		<resource-root path="httpcore-nio-${httpcore-nio.version}.jar" />
		<resource-root path="httpmime-${httpmime.version}.jar" />
	</resources>
	<dependencies>
		<module name="org.slf4j" />
		<module name="org.apache.commons.logging" />
		<system export="false">
			<paths>
				<path name="javax/naming" />
				<path name="javax/net/ssl" />
			</paths>
		</system>
	</dependencies>
</module>
